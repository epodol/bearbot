{
  "name": "bearbot",
  "version": "1.0.0",
  "description": "The Bear Bot Discord Bot",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "start": "rm -rf ./dist/ && tsc && npm-run-all --parallel node tsc",
    "watch": "rm -rf ./dist/ && tsc && npm-run-all --parallel node tsc:watch",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "node": "node --no-warnings .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epodol/bearbot.git"
  },
  "author": "epodol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/epodol/bearbot/issues"
  },
  "homepage": "https://github.com/epodol/bearbot",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  }
}
